%h1= t('backend.user.audit')
%hr/

%table.table.table-striped
    %thead
        %tr
            %th
            %th= t('user.classes')
            %th= t('user.name')
            %th= t('user.email')
            %th= t('user.phone')
    %tbody
        - @users.each do |user|
            %tr
                %td
                    %a{:href=> "verify/#{user.id}"} Audit
                %td= user.classes
                %td= user.name
                %td= user.email
                %td= user.phone



