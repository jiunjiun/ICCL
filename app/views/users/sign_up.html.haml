%h1 Sign Up
= form_for @user, :url=> sign_upping_path(@user), method: "post" do |f|
    = f.text_field :id, type: "hidden"

    = f.label :classes
    = f.select(:classes, options_for_select([["研究生", "master"], ["大學部", "university"]]))

    = f.label :name
    = f.text_field :name

    = f.label :email
    = f.text_field :email

    = f.label :phone
    = f.text_field :phone

    = f.submit "Sign Up"