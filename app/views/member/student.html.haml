#student
    .row
        %h1= t('member.student')
        %hr/
    .row.master
        %h3= t('member.master')
        %hr/
        - @Master.each do |master|
            .col-md-4
                .userprofile.row
                    .col-md-1.text-center
                        / %img.img-circle{:src => "https://graph.facebook.com/#{master.fb_id}/picture?type=square&width=90&height=90", :alt => "square", :width => 90, :height => 90}
                        %img.img-circle{:src => "holder.js/90x90", :alt => "square", :width => 90, :height => 90}
                    .col-md-6.col-md-offset-3
                        %h3
                            %strong= master.name
                        %h5= master.email
    .row
        %h3= t('member.university')
        %hr/
        - @University.each do |university|
            .col-md-4
                .userprofile.row
                    .col-md-1.text-center
                        / %img.img-circle{:src => "https://graph.facebook.com/#{university.fb_id}/picture?type=square&width=90&height=90", :alt => "square", :width => 90, :height => 90}
                        %img.img-circle{:src => "holder.js/90x90", :alt => "square", :width => 90, :height => 90}
                    .col-md-6.col-md-offset-3
                        %h3
                            %strong= university.name
                        %h5= university.email